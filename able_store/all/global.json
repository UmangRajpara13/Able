{
    "global": {
        "open-vs-code": {
            "cli": "code",
            "debug": false
        },
        "open-settings": {
            "cli": "gnome-control-center",
            "debug": false
        },
        "open-configuration": {
            "cli": "nautilus",
            "args": [
                "./able_store"
            ],
            "debug": false
        },
        "jenny": {
            "cli": "nautilus",
            "debug": false
        },
        "william": {
            "cli": "bash",
            "args": [
                "william.sh"
            ],
            "debug": false,
            "location": "/home/user/able_store/scripts"
        },
        "charlie": {
            "cli": "bash",
            "args": [
                "charlie.sh"
            ],
            "debug": false,
            "location": "/home/user/able_store/scripts"
        },
        "brightness": {
            "cli": "bash",
            "args": [
                "brightness.sh"
            ],
            "debug": true,
            "location": "/home/user/able_store/scripts"
        },
        "brightness-maximum": {
            "cli": "bash",
            "args": [
                "brightness-maximum.sh"
            ],
            "debug": true,
            "location": "/home/user/able_store/scripts"
        },
        "brightness-minimum": {
            "cli": "bash",
            "args": [
                "brightness-minimum.sh"
            ],
            "debug": true,
            "location": "/home/user/able_store/scripts"
        },
        "create-a-new-project": {
            "cli": "bash",
            "args": [
                "new-project.sh"
            ],
            "debug": true,
            "location": "/home/user/able_store/scripts"
        }
    }
}