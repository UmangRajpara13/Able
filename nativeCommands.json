{
    "global": {
        "open-downloads": {
            "paraphrases": [
                "open downloads"
            ],
            "action": {
                "cli": "nautilus",
                "args": [
                    "/home/user/Downloads"
                ],
                "debug": false
            }
        },
        "show-my-projects": {
            "paraphrases": [],
            "action": {
                "cli": "nautilus",
                "args": [
                    "/home/user/Desktop/My Projects"
                ],
                "debug": false
            }
        },
        "open-listening-service": {
            "paraphrases": [],
            "action": {
                "cli": "code",
                "args": [
                    "-r",
                    "."
                ],
                "location": "/home/user/able/whisper-mint",
                "debug": false
            }
        }
    }
}